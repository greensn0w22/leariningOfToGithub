#include "Uart2.h"
#include "UART.h"
#include "Macro.h"
#include "Clock.h"
#include "interrupt.h"
#include "PinsFunctions.h"

Uart2::Uart2(){}

Uart2::Uart2(
	TXD2 = P2_8,
	RXD2 = P2_9,
	BaudRate = _115_200,
	Parity = Disable,
	Interupt = Disable_Int){

	if(!allReadyDeclared){
		allReadyDeclared++;

		SB(24,PCONP);

		if(P2_8){
			I2B(PINSEL4,((P2_8&0xFFFE)>>1),(P2_8&0X000F));
		}


		if(P2_9){
			I2B(PINSEL4,((P2_9&0xFFFE)>>1),(P2_9&0X000F));
		}

	}


}
